{
  "name": "@repo/ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    "./Box": "./src/Box.tsx",
    "./FAQDropdown": "./src/FAQDropdown.tsx",
    "./StreetSign": "./src/StreetSign.tsx",
    "./LinkedInLogo": "./src/LinkedInLogo.tsx",
    "./ArrowButton": "./src/ArrowButton.tsx",
    "./TitleBox": "./src/TitleBox.tsx",
    "./Carousel": "./src/Carousel.tsx",
    "./PassportCard": "./src/PassportCard.tsx",
    "./Typography": "./src/Typography.tsx",
    "./Icons/*": "./src/Icons/*.tsx",
    "./PaginationDots": "./src/PaginationDots.tsx"
  },
  "license": "MIT",
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "lint": "eslint .",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@repo/util": "*"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@repo/eslint-config": "*",
    "@repo/tailwind-config": "*",
    "@repo/typescript-config": "*",
    "@types/react": "^18.2.61",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.14.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  }
}
